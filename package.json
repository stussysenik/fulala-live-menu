{
  "name": "fulala-live-menu",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "convex dev",
    "dev:all": "concurrently \"convex dev\" \"vite dev\"",
    "validate:env": "node scripts/validate-env.cjs",
    "build": "npm run validate:env && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "prepare": "husky || true",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/sveltekit": "^10.2.8",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-vercel": "^6.3.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^22",
    "@vitest/browser": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "playwright": "^1.58.2",
    "sharp": "^0.34.5",
    "storybook": "^10.2.8",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/cormorant-garamond": "^5.2.11",
    "@fontsource/dm-mono": "^5.2.7",
    "convex": "^1.10.0"
  }
}
